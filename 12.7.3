per_cent = {'ТКБ': 5.6, 'СКБ': 5.9, 'ВТБ': 4.28, 'СБЕР': 4.0}
money = input("Введите сумму средств на депозите  ")
money = float(money)
procent = per_cent.values()
deposit = list(map((money/100).__mul__, procent))
rounded = list(map(round, deposit))
max_number = round(max(deposit))
print("За год вы можете заработать на процентах -", rounded)
print("Максимальная сумма, которую вы можете заработать -", max_number)
